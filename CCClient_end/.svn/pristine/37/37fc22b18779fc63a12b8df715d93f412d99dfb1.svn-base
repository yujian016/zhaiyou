<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <LinearLayout
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/fragment_order1_myorder1"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="#1FADFF"
            android:gravity="center"
            android:text="私人超市订单"
            android:textColor="@color/white"
            android:textSize="14sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/col_235" />

        <TextView
            android:id="@+id/fragment_order1_myorder2"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="代理进货订单"
            android:textColor="@color/col_72"
            android:textSize="14sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/col_235" />

        <TextView
            android:id="@+id/fragment_order1_myorder3"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="店铺购物订单"
            android:textColor="@color/col_72"
            android:textSize="14sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/col_235" />

        <TextView
            android:id="@+id/fragment_order1_myorder4"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="潮流数码订单"
            android:textColor="@color/col_72"
            android:textSize="14sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/col_235" />

        <TextView
            android:id="@+id/fragment_order1_myorder5"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="二手交易订单"
            android:textColor="@color/col_72"
            android:textSize="14sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/col_235" />

        <TextView
            android:id="@+id/fragment_order1_myorder6"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="积分商城订单"
            android:textColor="@color/col_72"
            android:textSize="14sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/col_235" />

        <TextView
            android:id="@+id/fragment_order1_myorder7"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="校园购物订单"
            android:textColor="@color/col_72"
            android:textSize="14sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/col_235" />
    </LinearLayout>

    <View
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:background="@color/col_235" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <com.ccc.www.view.XListView
            android:id="@+id/order_fragment_order1_xlistview1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <com.ccc.www.view.XListView
            android:id="@+id/order_fragment_order1_xlistview2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />

        <com.ccc.www.view.XListView
            android:id="@+id/order_fragment_order1_xlistview3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />

        <com.ccc.www.view.XListView
            android:id="@+id/order_fragment_order1_xlistview4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />

        <com.ccc.www.view.XListView
            android:id="@+id/order_fragment_order1_xlistview5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />

        <com.ccc.www.view.XListView
            android:id="@+id/order_fragment_order1_xlistview6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />

        <com.ccc.www.view.XListView
            android:id="@+id/order_fragment_order1_xlistview7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />
    </LinearLayout>

</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:padding="30dp" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/corners_pop_proxystocktip_bg"
        android:orientation="vertical" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="15dp"
            android:text="信息确认"
            android:textColor="@color/col_72"
            android:textSize="15sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.6dp"
            android:background="@color/fildpwd_textbg_color" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/addsuggestcomment_ok"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="8dp"
                android:text="收货人"
                android:textColor="@color/col_72" />

            <View
                android:layout_width="0.6dp"
                android:layout_height="match_parent"
                android:background="@color/fildpwd_textbg_color" />

            <EditText
                android:id="@+id/pop_proxystocktip_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@null"
                android:gravity="center"
                android:hint="请输入收货人姓名"
                android:padding="8dp"
                android:textColor="@color/col_72"
                android:textSize="14sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.6dp"
            android:background="@color/fildpwd_textbg_color" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="8dp"
                android:text="联系电话"
                android:textColor="@color/col_72" />

            <View
                android:layout_width="0.6dp"
                android:layout_height="match_parent"
                android:background="@color/fildpwd_textbg_color" />

            <EditText
                android:id="@+id/pop_proxystocktip_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@null"
                android:gravity="center"
                android:hint="请输入联系电话"
                android:padding="8dp"
                android:textColor="@color/col_72"
                android:textSize="14sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.6dp"
            android:background="@color/fildpwd_textbg_color" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="8dp"
                android:text="收货地址"
                android:textColor="@color/col_72" />

            <View
                android:layout_width="0.6dp"
                android:layout_height="match_parent"
                android:background="@color/fildpwd_textbg_color" />

            <EditText
                android:id="@+id/pop_proxystocktip_addr"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@null"
                android:gravity="center"
                android:hint="请输入收货地址"
                android:padding="8dp"
                android:textColor="@color/col_72"
                android:textSize="14sp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.6dp"
            android:background="@color/fildpwd_textbg_color" />
 

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="8dp"
                android:text="兑换积分"
                android:textColor="@color/col_72" />

            <View
                android:layout_width="0.6dp"
                android:layout_height="match_parent"
                android:background="@color/fildpwd_textbg_color" />

            <TextView
                android:id="@+id/pop_proxystocktip_money"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:text="cancel"
                android:textColor="@color/col_72" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.6dp"
            android:background="@color/fildpwd_textbg_color" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/pop_proxystocktip_cancel"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:text="取消"
                android:textColor="@color/col_72" />

            <View
                android:layout_width="0.6dp"
                android:layout_height="match_parent"
                android:background="@color/fildpwd_textbg_color" />

            <TextView
                android:id="@+id/pop_proxystocktip_pay"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="8dp"
                android:text="立即兑换"
                android:textColor="@color/col_72" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout><!-- From: file:/D:/waibaoworkpace/CCClient_end/res/layout/pop_exchangepointtip.xml -->
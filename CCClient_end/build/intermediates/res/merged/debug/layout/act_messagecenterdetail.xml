<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/rl_title"
        style="@style/header_style" >

        <TextView
            android:id="@+id/tv_digital_header_title"
            style="@style/header_title_withshool_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="消息详情" />

        <ImageButton
            android:id="@+id/ib_digital_goback"
            style="@style/header_left_goback_style" />
    </RelativeLayout>

    <TextView
        android:id="@+id/act_messagecenterdetail_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="15dp"
        android:gravity="center"
        android:textColor="@color/col_72"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/act_messagecenterdetail_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_marginTop="20dp"
        android:gravity="center_vertical"
        android:textColor="@color/col_72"
        android:textSize="13sp" />

</LinearLayout><!-- From: file:/D:/waibaoworkpace/CCClient_end/res/layout/act_messagecenterdetail.xml -->
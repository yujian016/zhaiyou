<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/rl_title"
        style="@style/header_style" >

        <TextView
            android:id="@+id/tv_select_school_title"
            style="@style/header_title_tv_style"
            android:text="代理进货" />

        <ImageButton
            android:id="@+id/ib_select_school_goback"
            style="@style/header_left_goback_style" />

        <ImageView
            android:id="@+id/iv_proxysockcartcount"
             android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:paddingBottom="5dp"
            android:paddingLeft="5dp"
            android:paddingRight="15dp"
            android:paddingTop="5dp"
            android:src="@drawable/cart_white"/>
    </RelativeLayout>

    <com.viewpagerindicator.TabPageIndicator
        android:id="@+id/activity_proxysock_indicator"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="gone" />

    <android.support.v4.view.ViewPager
        android:id="@+id/activity_proxysock_viewpager"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1" />

</LinearLayout><!-- From: file:/D:/waibaoworkpace/CCClient_end/res/layout/activity_proxysock.xml -->